package fstream.utils {	/**	* Append a padding chars to the left for the provided string until the limit is reach.	* If the length of provided string is greater than the parameter length, original string is return.	* @param length Total length result must measure	* @param value String to pad.	*/	public function charLeftPadding(value:String, length:uint = 0, padding:String = " "):String	{		if(padding == null || padding.length == 0)			padding = " ";				while (value.length < length)			value = padding + value;		return value;	}}